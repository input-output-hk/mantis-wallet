{
  "name": "luna-wallet",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^12.13.0"
  },
  "main": "build/main/main.js",
  "homepage": "./",
  "scripts": {
    "start": "EXTEND_ESLINT=true react-app-rewired start",
    "build-renderer": "EXTEND_ESLINT=true react-app-rewired build",
    "build-main": "tsc -p ./tsconfig.main.json",
    "test": "EXTEND_ESLINT=true react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "yarn prettier-check && yarn eslint && yarn stylelint",
    "eslint": "eslint src --ext .js,.ts,.tsx",
    "stylelint": "stylelint '**/*.scss'",
    "prettier-check": "prettier --no-editorconfig --check \"**/*.{js,ts,tsx,scss}\"",
    "prettier": "prettier --no-editorconfig --write \"**/*.{js,ts,tsx,scss}\"",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "electron": "electron ./build/main/main.js",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron ./build/main/main.js",
    "build-all": "yarn build-renderer && yarn build-main",
    "package": "ts-node --project tsconfig.bin.json ./bin/package.ts",
    "package-win": "ts-node --project ./tsconfig.bin.json ./bin/package-win.ts",
    "build-dist": "yarn build-all && yarn package",
    "build-dist-win": "yarn build-all && yarn package-win"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && pretty-quick --staged --pattern \"**/*.*(js|ts|tsx|scss)\" && yarn lint"
    }
  },
  "dependencies": {
    "@types/big.js": "^4.0.5",
    "@types/classnames": "^2.2.9",
    "@types/convict": "^4.2.1",
    "@types/jest": "24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "12.12.14",
    "@types/qrcode.react": "^1.0.0",
    "@types/react": "16.9.14",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/vfile-message": "^2.0.0",
    "antd": "^3.26.6",
    "antd-scss-theme-plugin": "^1.0.8",
    "babel-plugin-import": "^1.13.0",
    "big.js": "^5.2.2",
    "classnames": "^2.2.6",
    "comlink": "^4.2.0",
    "convict": "^5.2.0",
    "customize-cra": "^0.9.1",
    "fp-ts": "^2.4.1",
    "js-file-download": "^0.4.9",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.0.0",
    "qrcode.react": "^1.0.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-inlinesvg": "^1.2.0",
    "react-polymorph": "^0.9.0-rc.24",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "redux": "^4.0.4",
    "rxjs": "^6.5.4",
    "typescript": "3.7.3",
    "unstated-next": "^1.1.0",
    "web3": "git+ssh://git@github.com:input-output-hk/midnight-web3.git#develop"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@electron-forge/cli": "6.0.0-beta.47",
    "@electron-forge/maker-deb": "6.0.0-beta.47",
    "@electron-forge/maker-rpm": "6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "6.0.0-beta.47",
    "@electron-forge/maker-zip": "6.0.0-beta.47",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-notes": "5.1.9",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/chai": "^4.2.7",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "electron-packager": "^14.2.0",
    "electron-prebuilt-compile": "4.0.0",
